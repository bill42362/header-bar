{
  "name": "header-bar-demo",
  "version": "1.0.0",
  "description": "Demo of 'header-bar'.",
  "main": "js/App.js",
  "scripts": {
    "rmdist": "rimraf dist",
    "buildjs": "babel src -d dist --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "buildcss": "lessc src/client/css/index.less dist/client/css/index.css",
    "build": "npm run rmdist && npm run buildjs && npm run buildcss",
    "start": "npm run build && cross-env NODE_ENV=production node dist/server",
    "devserver": "babel-node src/server",
    "devserver:auto": "nodemon --ignore dist --exec babel-node src/server",
    "wds": "webpack-dev-server --progress",
    "test": "jest"
  },
  "keywords": [
    "header",
    "rwd",
    "react"
  ],
  "author": "Pi-Yin Hsiao",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "es6-promise": "^4.2.2",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "header-bar": "*",
    "html-loader": "^0.5.4",
    "img-loader": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "nodemon": "^1.14.10",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  }
}
